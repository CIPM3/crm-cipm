{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:https": "next dev --experimental-https",
    "build": "next build",
    "build:netlify": "SKIP_ESLINT=true SKIP_TYPE_CHECK=true next build",
    "build:optimized": "node scripts/build-optimization.js",
    "build:next": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:debug": "next build --debug",
    "build:production": "NODE_ENV=production node scripts/build-optimization.js",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "export": "next export",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --max-warnings 0",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rm -rf .next out dist coverage",
    "clean:all": "rm -rf .next out dist coverage node_modules package-lock.json && npm install",
    "clean:cache": "rm -rf .next/cache && npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "perf:lighthouse": "npx lighthouse http://localhost:3000 --output=html --output-path=./reports/lighthouse.html",
    "perf:bundle": "npm run build:analyze && npx @next/bundle-analyzer .next",
    "perf:monitor": "node scripts/performance-monitor.js",
    "firebase:emulators": "firebase emulators:start",
    "firebase:deploy": "firebase deploy",
    "firebase:deploy:functions": "firebase deploy --only functions",
    "firebase:deploy:hosting": "firebase deploy --only hosting",
    "db:backup": "node scripts/backup-firestore.js",
    "db:seed": "node scripts/seed-database.js",
    "precommit": "npm run lint:fix && npm run type-check",
    "prepare": "husky install",
    "postinstall": "prisma generate || true",
    "security:audit": "npm audit --audit-level moderate",
    "security:check": "npx audit-ci --config audit-ci.json",
    "deps:update": "npx npm-check-updates -u",
    "deps:check": "npx npm-check-updates",
    "deps:unused": "npx depcheck",
    "migrate:from-old": "tsx scripts/migrate-from-old-firebase.ts",
    "migrate:from-old:dry": "tsx scripts/migrate-from-old-firebase.ts --dry-run",
    "upload:firebase": "tsx scripts/upload-to-firebase.ts",
    "upload:firebase:dry": "tsx scripts/upload-to-firebase.ts --dry-run",
    "upload:firebase:emulator": "tsx scripts/upload-to-firebase.ts --emulator",
    "analyze:bundle": "npm run build:analyze && npx @next/bundle-analyzer .next",
    "analyze:size": "npx size-limit",
    "optimize:images": "npx next-optimized-images",
    "validate:config": "node scripts/validate-config.js",
    "health:check": "node scripts/health-check.js",
    "deploy:staging": "npm run build:production && firebase deploy --project staging",
    "deploy:production": "npm run build:production && firebase deploy --project production"
  },
  "dependencies": {
    "@hookform/resolvers": "latest",
    "@netlify/plugin-nextjs": "^5.10.1",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "latest",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-avatar": "latest",
    "@radix-ui/react-checkbox": "latest",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "latest",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-label": "latest",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "latest",
    "@radix-ui/react-tabs": "latest",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "8.5.1",
    "firebase": "^11.10.0",
    "firebase-admin": "^13.5.0",
    "formik": "^2.4.6",
    "framer-motion": "^12.12.1",
    "gsap": "^3.13.0",
    "input-otp": "1.4.1",
    "lucide-react": "^0.454.0",
    "motion": "^12.18.1",
    "next": "14.2.16",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-day-picker": "8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "latest",
    "react-resizable-panels": "^2.1.7",
    "recharts": "latest",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "vaul": "^0.9.9",
    "yup": "^1.6.1",
    "zod": "latest",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@next/bundle-analyzer": "^14.2.16",
    "@playwright/test": "^1.40.0",
    "@tanstack/react-query-devtools": "^5.85.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^22.13.10",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "firebase-tools": "^12.9.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.3",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2"
  }
}
